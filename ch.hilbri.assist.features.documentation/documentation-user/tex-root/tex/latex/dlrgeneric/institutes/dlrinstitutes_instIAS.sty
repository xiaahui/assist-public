% Check, if dlrinstitutes is loaded
\ifx\dlrinstitutes@loaded\undefined%
	\PackageWarningNoLine{dlrinstitutes}{Loading data file without package dlrinstitutes}%
\fi%
%
% Load default site definition
\dlrinstitutes@def         {site}        {GO}%
%
% Define default (english) data fields
\dlrinstitutes@def         {abbreviation}{IAS}%
\dlrinstitutes@def         {name}        {Institute for Aerodynamics and Flow Technology}%
\dlrinstitutes@def         {department}  {}%
\dlrinstitutes@def         {head}        {Prof. Dr. Dr. Andreas Dillmann}%
\dlrinstitutes@def         {operator}    {2177}%
\dlrinstitutes@def         {fax}         {2889}%
\dlrinstitutes@def         {url}         {http://www.dlr.de/as/en}%
%
% Override language specific data fields (ngerman)
\dlrinstitutes@def[ngerman]{name}        {Institut f\"ur Aerodynamik und Str\"omungstechnik}%
\dlrinstitutes@def[ngerman]{url}         {http://www.dlr.de/as}%
%
% Override language specific data fields (german)
\dlrinstitutes@def[german] {name}        {Institut f\"ur Aerodynamik und Str\"omungstechnik}%
\dlrinstitutes@def[german] {url}         {http://www.dlr.de/as}%
